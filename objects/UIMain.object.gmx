active_tab=t_general;

var element;
var spacing=16;
var legal_x=CW+32;
var legal_y=128;
var legal_width=ui_legal_width();
var col_width=legal_width/2-spacing/2;

/*
 * general tab setup
 */

var yy=legal_y+spacing;

element=create_radio_array(legal_x+spacing, yy, "Selection mode", col_width, element_height, uivc_radio_selection_mode, Camera.selection_mode);
create_radio_array_options(element, "Single", "Rectangle", "Circle");
ds_list_add(t_general.contents, element);

yy=yy+ui_get_radio_array_height(element)+spacing;

element=create_checkbox(legal_x+spacing, yy, "Add to Selection", col_width, element_height, uivc_check_selection_addition, Camera.selection_addition);
ds_list_add(t_general.contents, element);

enum UITabs {
    GENERAL
}

instance_deactivate_object(UIThing);
