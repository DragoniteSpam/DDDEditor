var t_general=create_tab("General", UITabs.GENERAL, 0, id);
var t_1=create_tab("T1", 1, 0, id);
var t_2=create_tab("T2", 2, 0, id);

var t_3=create_tab("T3", 3, 1, id);
var t_4=create_tab("T4", 4, 1, id);
var t_5=create_tab("T5", 5, 1, id);
var t_6=create_tab("T6", 6, 1, id);

// the game will crash if you create a tab row with zero width.
